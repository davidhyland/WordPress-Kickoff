{
  "name": "davidhyland/wp-plugin-bundle",
  "type": "project",
  "require": {
    "wpackagist-plugin/acf-content-analysis-for-yoast-seo": "*",
    "wpackagist-plugin/all-in-one-wp-migration": "*",
    "wpackagist-plugin/classic-editor": "*",
    "wpackagist-plugin/performance-lab": "*",
    "wpackagist-plugin/safe-svg": "*",
    "wpackagist-plugin/sg-cachepress": "*",
    "wpackagist-plugin/sg-security": "*",
    "wpackagist-plugin/simple-history": "*",
    "wpackagist-plugin/updraftplus": "*",
    "wpackagist-plugin/version-info": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/accessibility-onetap": "*",
    "wpengine/advanced-custom-fields-pro": "*",
    "gravity/gravityforms": "*",
    "gravity/gravityformsrecaptcha": "*",
    "davidhyland/all-in-one-wp-migration-unlimited-extension": "dev-master",
    "davidhyland/happyfiles-pro": "dev-master",
    "davidhyland/wpconsent-premium": "dev-master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    },
    {
      "type": "composer",
      "url": "https://composer.gravity.io"
    },
    {
      "type": "vcs",
      "url": "https://github.com/davidhyland/all-in-one-wp-migration-unlimited-extension.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/davidhyland/happyfiles-pro.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/davidhyland/wpconsent-premium.git"
    }
  ],
  "extra": {
    "installer-paths": {
      "content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "php-stubs/wordpress-stubs": "^6.8",
    "szepeviktor/phpstan-wordpress": "^2.0",
    "php-stubs/acf-pro-stubs": "^6.5"
  }
}


